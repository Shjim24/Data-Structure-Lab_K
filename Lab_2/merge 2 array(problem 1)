#include<bits/stdc++.h>
using namespace std;
int main()
{

    int array1[5];
    int array2[8];
    int Merge[13];
    int j=5;
    int k=4;
    for(int i=0; i<5; i++)
    {
        cin>>array1[i];
        Merge[k]=array1[i];
        k--;
    }
    cout<<"Input another array:"<<endl;
    for(int i=0; i<8; i++)
    {
        cin>>array2[i];
        Merge[j]=array2[i];
        j++;
    }

    for(int i=13; i>=0; i--)
    {
        cout<<Merge[i]<<" ";
    }
}



